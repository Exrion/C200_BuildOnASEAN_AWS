@{
    ViewData["Title"] = "Analytics | Admin";
}


<h2> Training Data</h2>

<div class="row">
    <div class="col-sm-4">
        <div>
            <img src="~/Images/Shared/plot.png"/>
        </div>
        <div>
            Validation Accuracy: @ViewData["ValAcc"] 
        </div>
        <div>
            Validation Loss: @ViewData["ValLoss"]
        </div>
        <div>
            Training Accuracy: @ViewData["TrainAcc"]
        </div>
        <div>
            Training Loss: @ViewData["TrainLoss"] 
        </div>
    </div>

</div>